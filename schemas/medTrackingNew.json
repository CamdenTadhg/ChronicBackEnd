            "format": "date",
            "formatMaximum": "now"
        },
        "timeOfDay": {
            "type": "string",
            "enum": ["AM", "Midday", "PM", "Evening"]
        },
        "number": {
            "type": "number",
            "minimum": 0
        }
    },
    "additionalProperties": false,
    "required": [
        "medId",
        "trackDate",
        "timeOfDay", 
        "number"
    ]
}